{
    "openapi": "3.0.0",
    "info": {
        "title": "GemeindeOnline API",
        "version": "0.1.0",
        "description": "Web API for gemeindeonline.ch website and related services",
        "contact": {
            "name": "Baltic IT Solutions OÃœ",
            "email": "info@bits.ee"
        }
    },
    "servers": [
        {
            "url": "/api/",
            "description": "Zahhar Local environment"
        },
        {
            "url": "/gena/api/",
            "description": "Ahmed Local environment"
        }
    ],
    "paths": {
        "/locations": {
            "get": {
                "summary": "All locations with services",
                "tags": [ "Locations" ],
                "description": "Lists locations that offer at least 1 service.",
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/locations/{zipcode}/services": {
            "get": {
                "summary": "Services in given location",
                "tags": [ "Locations" ],
                "description": "Lists all services offered in the given location.",
                "parameters": [
                    {
                        "in": "path",
                        "name": "zipcode",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "minimum": 1000,
                            "maximum": 9999
                        },
                        "description": "Swiss ZIP-code, 4 digits in 1000-9999."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/auth/email": {
            "post": {
                "summary": "Sign in/up to website",
                "description": "Sign in/up to website",
                "tags": [ "Emails" ],
                "operationId": "InsertWithForm",
                "required": true,
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "token": {
                                        "type": "string"

                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/auth/email/verify/{key}": {
            "get": {
                "summary": "Key verifying",
                "tags": [ "Emails" ],
                "description": "Response",
                "parameters": [
                    {
                        "in": "path",
                        "name": "key",
                        "required": true,
                        "description": "key is uniid"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        }
    }
}
